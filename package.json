{
  "name": "@mukul-karnwal/mkd-ui",
  "version": "0.1.0",
  "description": "Next.js friendly UI library by Mukul Karnwal",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "tsup": "^8",
    "typescript": "^5"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "next": ">=13",
    "react-icons": ">=4"
  },
  "license": "MIT"
}